<toolkit:Bootstrapper x:Class="SoftwareKobo.U148.App"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:local="using:SoftwareKobo.U148"
                      xmlns:toolkit="using:SoftwareKobo.UniversalToolkit"
                      xmlns:vm="using:SoftwareKobo.U148.ViewModels"
                      xmlns:datas="using:SoftwareKobo.U148.Datas">
    <toolkit:Bootstrapper.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" />
            <datas:AppSettings x:Key="Settings" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/AppStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </toolkit:Bootstrapper.Resources>
</toolkit:Bootstrapper>